{
  "expo": {
    "name": "Doiti",
    "slug": "doiti",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/owl-mascot.png",
    "scheme": "doiti",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-hero.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/owl-mascot.png",
        "backgroundColor": "#0f172a"
      },
      "package": "com.doiti.app"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e3be79f0-2914-4f39-ae9d-6b57fd4b3211"
      }
    },
    "owner": "bruno1308"
  }
}
